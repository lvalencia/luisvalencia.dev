{
  "name": "@luvle/code-deploy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "ts-node ./src/main.ts",
    "deploy:poll": "POLL=true ts-node ./main.ts",
    "release": "run-s test deploy",
    "test": "ts-mocha --package ./tsconfig.json --paths ./test/**/*.spec.ts",
    "test:watch": "ts-mocha --package ./tsconfig.json --paths ./test/**/*.spec.ts -w --watch-files 'src/**/*.ts' --watch-files 'test/**/*.spec.ts'"
  },
  "dependencies": {
    "@luvle/utils": "0.1.1",
    "aws-sdk": "^2.1272.0",
    "mime-types": "^2.1.35",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.9",
    "@types/sinon": "^10.0.13",
    "@types/underscore": "^1.11.4",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
