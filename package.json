{
  "name": "luisvalencia.dev",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvalencia/luisvalencia.dev.git"
  },
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "clean": "rimraf ./dist",
    "dev": "vite",
    "deploy": "ts-node ./deploy/main.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint --fix . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "release": "run-s clean build test deploy",
    "test": "run-p test:unit test:e2e:headless",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:unit:watch": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "test:e2e:headless": "CI=true playwright test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "latest-3",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "pinia": "^2.0.26",
    "vue": "^3.2.45",
    "vue-i18n": "9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-dom": "^3.2.45",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/runtime-dom": "^3.2.45",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "aws-sdk": "^2.1272.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "mime-types": "^2.1.35",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3",
    "vite": "^4.0.0",
    "vite-plugin-inject-preload": "^1.1.2",
    "vitest": "^0.25.3",
    "vue-demi": "^0.13.11",
    "vue-tsc": "^1.0.9"
  }
}
