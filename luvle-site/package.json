{
  "name": "@luvle/luisvalencia.dev",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "clean": "rimraf ./dist",
    "dev": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint --fix . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "release": "CI=true run-s clean build test",
    "test": "run-p test:unit test:e2e:headless",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:unit:watch": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "test:e2e:headless": "CI=true playwright test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@luvle/utils": "workspace:^",
    "howler": "^2.2.4",
    "moment": "^2.30.1",
    "pinia": "^3.0.4",
    "three": "^0.182.0",
    "troika-three-text": "^0.52.4",
    "underscore": "^1.13.7",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.57.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/howler": "^2.2.12",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.7",
    "@types/three": "^0.182.0",
    "@types/underscore": "^1.13.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/compiler-dom": "^3.5.26",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/runtime-dom": "^3.5.26",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sass": "^1.97.2",
    "typescript": "^5.9.3",
    "unplugin-inject-preload": "^3.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "vue-demi": "^0.14.10",
    "vue-tsc": "^3.2.2"
  }
}
